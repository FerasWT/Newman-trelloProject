  "item": [
    {
      "name": "Create board",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "const jsonData = pm.response.json();\r",
              "\r",
              "pm.test(\"Status code is 200\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "});\r",
              "\r",
              "pm.test(\"Board not closed\", function () {\r",
              "    pm.expect(jsonData.closed).to.eql(false);\r",
              "});\r",
              "pm.test(\"Board not Pined\", function () {\r",
              "    pm.expect(jsonData.pinned).to.eql(false);\r",
              "});\r",
              "pm.test(\"private prmission level\", function () {\r",
              "    pm.expect(jsonData.prefs.permissionLevel).to.eql(\"private\");\r",
              "});\r",
              "\r",
              "\r",
              "pm.environment.set(\"Boarder_ID\", jsonData.id);\r",
              "\r",
              ""
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        },
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "pm.environment.set(\"boardName\", \"Feras Board NO: \" + parseInt(Math.random()*10000));"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
      ],
